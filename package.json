{
  "name": "karpatkey-nc-aum-api",
  "version": "0.1.0",
  "description": "API to obtain some metrics of the assets under management, twitter posts and blog posts from mirror.xyz",
  "author": "Mariano Aguero <mariano@karpatkey.com>",
  "license": "MIT",
  "main": "dist/src/index.js",
  "engines": {
    "npm": ">=8.19.0",
    "yarn": ">=1.22.19",
    "node": ">=v18.13.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --watch .env, src src/index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "lint": "npx eslint src",
    "bulk:posts": "ts-node --project ./tsconfig.json ./src/scripts/bulkMirrorPosts.ts",
    "dump:all:tweets": "ts-node --project ./tsconfig.json ./src/scripts/dumpAllTweets.ts",
    "dump:daily:tweets": "ts-node --project ./tsconfig.json ./src/scripts/dumpDailyTweets.ts"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "excerpt-html": "^1.2.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "memory-cache": "^0.2.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "needle": "^3.3.1",
    "node-cron": "^3.0.3",
    "numbro": "^2.4.0",
    "showdown": "^2.1.0",
    "twitter-text": "^3.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/memory-cache": "^0.2.5",
    "@types/morgan": "^1.9.9",
    "@types/needle": "^3.3.0",
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/showdown": "^2.0.6",
    "@types/supertest": "^6.0.2",
    "@types/twitter-text": "^3.1.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
