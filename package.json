  "license": "MIT",
  "main": "dist/src/index.js",
  "engines": {
    "npm": ">=8.19.0",
    "yarn": ">=1.22.19",
    "node": ">=v18.13.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
